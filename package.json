    "@types/mocha": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.6.0",
    "husky": "^4.2.5",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7"
  }
