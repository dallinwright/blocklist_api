{
  "name": "blocklist-api",
  "version": "1.0.2",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.8.0",
    "@microsoft/tsdoc": "^0.12.20",
    "@types/aws-lambda": "^8.10.59",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.730.0",
